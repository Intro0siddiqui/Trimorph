{
  "name": "trimorph-manager",
  "displayName": "Trimorph Manager",
  "description": "Manage Trimorph jails directly from the VS Code status bar.",
  "version": "0.1.0",
  "publisher": "trimorph",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "trimorph.switchJail",
        "title": "Trimorph: Switch Active Jail"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.7.4"
  }
}